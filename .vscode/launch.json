{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "vuejs: chrome",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}/frontend",
      "breakOnLoad": true,
      "preLaunchTask": "docker local",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      },
      "runtimeExecutable": "/bin/google-chrome-beta",
      "runtimeArgs": [
        // "--new-tab",
        "-user-data-dir=\"/${workspaceFolder}/DevProfile\"",
        "--remote-debugging-port=9222",
        "--disable-background-networking"
      ]
    }
    // {
    //   "type": "chrome",
    //   "request": "launch",
    //   "name": "vuejs: chrome",
    //   "url": "http://localhost:8080",
    //   "webRoot": "${workspaceFolder}/src",
    //   "pathMappings": [{ "url": "webpack:///src/", "path": "${webRoot}/" }],
    //   "preLaunchTask": "cd server && . local.sh"
    // }
  ]
}
