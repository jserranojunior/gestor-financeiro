{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "vuejs: chrome",
      "url": "http://frontend.localhost",
      "webRoot": "${workspaceFolder}/frontend",
      "breakOnLoad": true,
      "preLaunchTask": "docker local",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      },
      "runtimeExecutable": "/bin/google-chrome-beta",
      "runtimeArgs": [
        // "--new-tab",
        "-user-data-dir=\"/${workspaceFolder}/DevProfile\"",
        "--remote-debugging-port=9222",
        "--disable-background-networking"
      ]
    }
  ]
}
