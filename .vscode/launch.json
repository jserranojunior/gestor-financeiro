{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "protocol": "inspector",
      "skipFiles": [
        "${workspaceFolder}/backend/node_modules/**/*.js",
        "<node_internals>/**/*.js",
        "${workspaceFolder}/backend/<node_internals>/**/*.js",
        "<node_internals>"
      ],
      "localRoot": "${workspaceFolder}/backend",
      "remoteRoot": "/app",
      "restart": true
    },
    {
      "type": "node",

      "request": "attach",

      "name": "Attach by Process ID",

      "processId": "${command:PickProcess}",

      "protocol": "inspector",

      "restart": true
    }
  ]
}
