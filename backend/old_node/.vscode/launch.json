{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "restart": true
    },
    {
      "name": "Launch frontend",
      "type": "chrome",
      "request": "launch",
      "url": "http://frontend.localhost",
      "webRoot": "./Resources/js",
      "runtimeExecutable": "/bin/google-chrome-beta"
    }
  ]
}