{
  "git.ignoreLimitWarning": true,
  "editor.formatOnSave": true,
  "explorerTerminal.showDockerCommands": true,
  "explorerTerminal.commands": [
    {
      "name": "ls",
      "command": "ls"
    },
    {
      "name": "Docker UP",
      "command": "cd server/ && docker-compose up -d"
    },
    {
      "name": "Docker Restart",
      "command": "cd server/ && docker-compose restart"
    },
    {
      "name": "Docker Down",
      "command": "cd server/ && docker-compose down"
    }
  ]
}
